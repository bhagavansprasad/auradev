from bs4 import BeautifulSoup
import urllib
import socket

searchurl = "http://www.airtel.in/smartbyte-s/page.html"
r = urllib.urlopen(searchurl).read()
soup = BeautifulSoup(r, "lxml")
print (soup)
print "1.--------------"

frame = soup.find_all("iframe")
print (frame)
print "2.--------------"
print (frame.find("ifame", "src="))


